<h1>Delete Tag</h1>

<form method="post">
    @foreach (var tag in ViewBag.tags)
    {
        <div>
            <label>
                <span>@tag.Name</span>
                <input type="checkbox" name="tagIds" value="@tag.Id">
            </label>
        </div>
    }

    <input type="submit" value="Delete Selected Tags" class="btn btn-danger">

</form>